@model TestNotebook.Models.Result

@{
    ViewBag.Title = "Details";
}

<div class="container">
    <h1>Result</h1>
    <h4>Details</h4>
    <div class="card" style="width: 18rem;">
        <div class="card-body">
            <h5 class="card-title">@Html.DisplayNameFor(model => model.description)</h5>
            <h6 class="card-subtitle mb-2 text-muted">@Html.DisplayFor(model => model.description)</h6>
            <p class="card-text">*</p>
            <div class="btn-group">
                    @Html.ActionLink("Edit", "Edit", new { id = Model.Id }, new {@class="btn btn-primary"})
                    @Html.ActionLink("Back to List", "Index",new {}, new {@class="btn btn-primary"})
            </div>
        </div>
    </div>
</div>